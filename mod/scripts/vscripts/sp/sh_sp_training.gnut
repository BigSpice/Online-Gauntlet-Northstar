void function GET_Successful( response, callback ){


}
void function GET_Fail( response, callback )
{


}
void function SEND_REQUEST(){
HttpRequest request
request.method = HttpRequestMethod.GET
request.url = "http://titanfall.rocks/gauntlet/"
request.queryParameters[ "name" ] <- [ name.tostring() ]
request.queryParameters[ "time" ] <- [ time.tostring() ]


void functionref( HttpRequestResponse ) onSuccess = void function ( HttpRequestResponse response ) : ( callback )
{
        GET_Successful( response, callback )
}

void functionref( HttpRequestFailure ) onFailure = void function ( HttpRequestFailure failure ) : ( callback )
{
        GET_Fail( failure, callback )
}

return NSHttpRequest( request, onSuccess, onFailure )

}
catch( error )
	{
		print( "Eval breakpoint error at SEND_REQUEST() at - sh_sp_training.nut: " + error + "\n" )
	}
}
